loadstring(game:HttpGet("https://raw.githubusercontent.com/Something478/DevTools/main/Tag"))()

local players = game:GetService("Players")
local lighting = game:GetService("Lighting")
local tweenService = game:GetService("TweenService")
local localPlayer = players.LocalPlayer
local playerPosition = localPlayer.Character and localPlayer.Character:FindFirstChild("HumanoidRootPart") and localPlayer.Character.HumanoidRootPart.Position or Vector3.new(0, 0, 0)
local teleportCFrame = CFrame.new(playerPosition.X, playerPosition.Y + 502, playerPosition.Z)

local originalLighting = {
    Ambient = lighting.Ambient,
    OutdoorAmbient = lighting.OutdoorAmbient,
    FogColor = lighting.FogColor,
    FogEnd = lighting.FogEnd,
    Brightness = lighting.Brightness
}

lighting.Ambient = Color3.fromRGB(40, 40, 70)
lighting.OutdoorAmbient = Color3.fromRGB(40, 40, 70)
lighting.FogColor = Color3.fromRGB(5, 5, 15)
lighting.FogEnd = 2000
lighting.Brightness = 0.25

local sky = lighting:FindFirstChildOfClass("Sky") or Instance.new("Sky")
sky.SkyboxBk = "http://www.roblox.com/asset/?id=159454299"
sky.SkyboxDn = "http://www.roblox.com/asset/?id=159454299"
sky.SkyboxFt = "http://www.roblox.com/asset/?id=159454299"
sky.SkyboxLf = "http://www.roblox.com/asset/?id=159454299"
sky.SkyboxRt = "http://www.roblox.com/asset/?id=159454299"
sky.SkyboxUp = "http://www.roblox.com/asset/?id=159454299"
sky.Parent = lighting

for _, obj in pairs(workspace:GetChildren()) do
    if obj.Name == "PlasmaMapIsland" then
        obj:Destroy()
    end
end

local island = Instance.new("Model")
island.Name = "PlasmaMapIsland"
island.Parent = workspace

local platform = Instance.new("Part")
platform.Name = "NebulaPlatform"
platform.Size = Vector3.new(600, 20, 600)
platform.Position = Vector3.new(playerPosition.X, playerPosition.Y + 500, playerPosition.Z)
platform.Anchored = true
platform.Material = Enum.Material.Neon
platform.Color = Color3.fromRGB(20, 5, 40)

local surfaceGui = Instance.new("SurfaceGui")
surfaceGui.Face = Enum.NormalId.Top
surfaceGui.Parent = platform

local frame = Instance.new("Frame")
frame.Size = UDim2.new(1, 0, 1, 0)
frame.BackgroundColor3 = Color3.fromRGB(20, 5, 40)
frame.BorderSizePixel = 0
frame.Parent = surfaceGui

for i = 1, 100 do
    local star = Instance.new("Frame")
    star.Size = UDim2.new(0, math.random(2, 10), 0, math.random(2, 10))
    star.Position = UDim2.new(0, math.random(0, 580), 0, math.random(0, 580))
    star.BackgroundColor3 = Color3.fromRGB(
        math.random(150, 255),
        math.random(150, 255),
        math.random(200, 255)
    )
    star.BorderSizePixel = 0
    star.Parent = frame
    
    spawn(function()
        while star.Parent do
            wait(math.random(5, 15)/10)
            star.Visible = not star.Visible
        end
    end)
end

platform.Parent = island

for i = 1, 50 do
    local crystal = Instance.new("Part")
    crystal.Shape = Enum.PartType.Ball
    crystal.Size = Vector3.new(math.random(10, 25), math.random(10, 25), math.random(10, 25))
    crystal.Position = Vector3.new(
        playerPosition.X + math.random(-250, 250),
        playerPosition.Y + 500 + math.random(20, 100),
        playerPosition.Z + math.random(-250, 250)
    )
    crystal.Anchored = true
    crystal.Material = Enum.Material.Glass
    crystal.Color = Color3.fromRGB(
        math.random(100, 200),
        math.random(50, 150),
        math.random(200, 255)
    )
    
    local pointLight = Instance.new("PointLight")
    pointLight.Brightness = 4
    pointLight.Range = 25
    pointLight.Color = crystal.Color
    pointLight.Parent = crystal
    
    crystal.Parent = island
end

