shadowToggle.Size = UDim2.new(0, 180, 0, 30)
shadowToggle.Position = UDim2.new(0.5, -90, 0, 120)
shadowToggle.BackgroundColor3 = Color3.fromRGB(100, 0, 150)
shadowToggle.BorderSizePixel = 0
shadowToggle.Text = "SHADOWS: ON"
shadowToggle.TextColor3 = Color3.new(1, 1, 1)
shadowToggle.Font = Enum.Font.SciFi
shadowToggle.TextSize = 14
shadowToggle.Parent = mainFrame

local minimizeButton = Instance.new("TextButton")
minimizeButton.Size = UDim2.new(0, 30, 0, 20)
minimizeButton.Position = UDim2.new(1, -35, 0, 5)
minimizeButton.BackgroundColor3 = Color3.fromRGB(100, 0, 0)
minimizeButton.BorderSizePixel = 0
minimizeButton.Text = "X"
minimizeButton.TextColor3 = Color3.new(1, 1, 1)
minimizeButton.Font = Enum.Font.SciFi
minimizeButton.TextSize = 14
minimizeButton.Parent = mainFrame

local showButton = Instance.new("TextButton")
showButton.Size = UDim2.new(0, 100, 0, 30)
showButton.Position = UDim2.new(0, 10, 1, -35)
showButton.BackgroundColor3 = Color3.fromRGB(0, 100, 0)
showButton.BorderSizePixel = 0
showButton.Text = "Show UI"
showButton.TextColor3 = Color3.new(1, 1, 1)
showButton.Font = Enum.Font.SciFi
showButton.TextSize = 14
showButton.Visible = false
showButton.Parent = screenGui

local isUIVisible = true
local shadowsEnabled = true

local function toggleShadows()
    shadowsEnabled = not shadowsEnabled
    
    if shadowsEnabled then
        lighting.Ambient = Color3.fromRGB(40, 40, 70)
        lighting.OutdoorAmbient = Color3.fromRGB(40, 40, 70)
        lighting.FogColor = Color3.fromRGB(5, 5, 15)
        lighting.FogEnd = 2000
        lighting.Brightness = 0.25
        shadowToggle.Text = "SHADOWS: ON"
        shadowToggle.BackgroundColor3 = Color3.fromRGB(100, 0, 150)
    else
        lighting.Ambient = originalLighting.Ambient
        lighting.OutdoorAmbient = originalLighting.OutdoorAmbient
        lighting.FogColor = originalLighting.FogColor
        lighting.FogEnd = originalLighting.FogEnd
        lighting.Brightness = originalLighting.Brightness
        shadowToggle.Text = "SHADOWS: OFF"
        shadowToggle.BackgroundColor3 = Color3.fromRGB(50, 50, 100)
    end
end

shadowToggle.MouseButton1Click:Connect(toggleShadows)

minimizeButton.MouseButton1Click:Connect(function()
    isUIVisible = false
    mainFrame.Visible = false
    showButton.Visible = true
end)

showButton.MouseButton1Click:Connect(function()
    isUIVisible = true
    mainFrame.Visible = true
    showButton.Visible = false
end)

button.MouseEnter:Connect(function()
    button.BackgroundColor3 = Color3.fromRGB(0, 150, 255)
    buttonGlow.Thickness = 3
end)

button.MouseLeave:Connect(function()
    button.BackgroundColor3 = Color3.fromRGB(0, 100, 200)
    buttonGlow.Thickness = 2
end)

button.MouseButton1Click:Connect(function()
    if localPlayer and localPlayer.Character then
        local humanoid = localPlayer.Character:FindFirstChild("HumanoidRootPart")
        if humanoid then
            humanoid.CFrame = teleportCFrame
        end
    end
end)

spawn(function()
    while true do
        if isUIVisible then
            for i = 1, 20 do
                mainFrame.Position = UDim2.new(0, 10, 0.5, -125 + math.sin(tick())*2)
                wait(0.05)
            end
        else
            wait(0.1)
        end
    end
end)

local function createFloatingPlanets()
    for i = 1, 8 do
        local planet = Instance.new("Part")
        planet.Shape = Enum.PartType.Ball
        planet.Size = Vector3.new(math.random(40, 80), math.random(40, 80), math.random(40, 80))
        planet.Position = Vector3.new(
            playerPosition.X + math.random(-800, 800),
            playerPosition.Y + 500 + math.random(250, 600),
            playerPosition.Z + math.random(-800, 800)
        )
        planet.Anchored = true
        planet.Material = Enum.Material.Neon
        planet.Color = Color3.fromRGB(
            math.random(50, 200),
            math.random(50, 200),
            math.random(50, 200)
        )
        
        local planetLight = Instance.new("PointLight")
        planetLight.Brightness = 3
        planetLight.Range = 40
        planetLight.Color = planet.Color
        planetLight.Parent = planet
        
        planet.Parent = island
        
        spawn(function()
            while planet.Parent do
                planet.CFrame = planet.CFrame * CFrame.Angles(0, math.rad(0.5), 0)
                wait()
            end
        end)
    end
end

createFloatingPlanets()