    local rock = Instance.new("Part")
    rock.Size = Vector3.new(
        math.random(10, 30),
        math.random(10, 30),
        math.random(10, 30)
    )
    rock.Position = Vector3.new(
        playerPosition.X + math.random(-350, 350),
        playerPosition.Y + 500 + math.random(-40, 200),
        playerPosition.Z + math.random(-350, 350)
    )
    rock.Anchored = true
    rock.Material = Enum.Material.Slate
    rock.Color = Color3.fromRGB(30, 30, 50)
